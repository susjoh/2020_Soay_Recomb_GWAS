pages = {e63},
pmid = {15736976},
title = {{Recombination difference between sexes: a role for haploid selection.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1044830{\&}tool=pmcentrez{\&}rendertype=abstract},
